{
  "name": "silex",
  "description": "Free and easy website builder for everyone.",
  "version": "2.5.0",
  "version:frontend": "2.10",
  "version:backwardcompat": "2.2.10",
  "author": "Alex Hoyau <a.hoyau@silexlabs.org> (https://lexoyo.me/)",
  "contributors": [
    "Jean-Baptiste Richardet <jeanbaptiste.richardet@gmail.com> (https://github.com/JbIPS)"
  ],
  "main": "dist/server/server/silex_web.js",
  "license": "GPL-3.0",
  "engines": {
    "node": "8.10.x",
    "npm": "5.6.x"
  },
  "precommit": [
    "lint:js",
    "lint:server"
  ],
  "scripts": {
    "webpack": "webpack --config webpack.config.js",
    "start": "node dist/server/server/silex_web.js",
    "start:debug": "cross-env SILEX_DEBUG=true nodemon dist/server/server/silex_web.js",
    "start:electron": "SILEX_ELECTRON=true electron .",
    "build": "npm run workaround-stage && npm run build:css:release && npm run build:html && npm run build:prodotype && npm run build:client:release && npm run build:server:release",
    "release": "electron-builder",
    "build:prodotype": "mkdir -p dist/prodotype/components &&  prodotype src/prodotype/components dist/prodotype/components/ && prodotype src/prodotype/styles dist/prodotype/styles/",
    "build:css:release": "lessc src/css/_styles.less dist/html/css/admin.css",
    "build:css:debug": "lessc src/css/_styles.less dist/html/css/admin.css --source-map",
    "build:html": "pug src/html/index.jade --out dist/html/ --no-debug",
    "build:html:debug": "pug src/html/debug.jade --out dist/html/",
    "build:client:release": "webpack --config webpack.config.js",
    "build:client:debug": "webpack --config webpack.config.js --watch",
    "build:server:release": "npm run workaround-babel && tsc -p tsconfig-server-release.json",
    "build:server:debug": "npm run workaround-babel && tsc -p tsconfig-server-debug.json",
    "postinstall": "npm run build",
    "workaround-babel": "rm -rf `node_modules`/@types/babel__*",
    "note: workaround-babel issue:": "https://github.com/facebook/jest/issues/8218",
    "workaround-stage": "cd node_modules/stage/ && npm i && cd ../../",
    "note: workaround-stage issue:": "this needs to be done because redux package is not found when installing stage normally"
  },
  "dependencies": {
    "redux": "^4.0.1",
    "stage": "github:lexoyo/stage#master",
    "ace-builds": "^1.4.3",
    "alertifyjs": "^1.11.2",
    "bluebird": "3.5.1",
    "body-parser": "^1.18.2",
    "clang-format": "^1.2.4",
    "cloud-explorer": "github:silexlabs/CloudExplorer2#master",
    "compression": "1.7.2",
    "cookie-parser": "1.4.3",
    "cookie-session": "2.0.0-beta.3",
    "cross-env": "5.1.4",
    "express": "4.16.3",
    "express-force-ssl": "0.3.2",
    "font-awesome": "^4.7.0",
    "js-beautify": "^1.9.1",
    "jsdom": "11.6.2",
    "less": "3.0.1",
    "node_modules-path": "github:lexoyo/node_modules-path#master",
    "nodemon": "^1.18.10",
    "normalize.css": "^8.0.1",
    "prodotype": "github:silexlabs/Prodotype#master",
    "promise-sequential": "1.1.1",
    "pug-cli": "^1.0.0-alpha6",
    "request": "2.85.0",
    "serve-static": "1.13.2",
    "silex-blank-templates": "github:silexlabs/silex-blank-templates#59b55d32a7829e004de6a9dd96c1af2d30ee4b6a",
    "silex-templates": "github:silexlabs/silex-templates#85d84f533bbd8d3234c314284bc64c7a114de606",
    "typescript": "^3.3.3333",
    "wysihtml": "^0.6.0-beta1"
  },
  "devDependencies": {
    "@types/redux": "^3.6.31",
    "@types/ace": "0.0.42",
    "@types/node": "^11.11.7",
    "electron": "^1.8.8",
    "electron-builder": "^20.28.2",
    "eslint": "^5.10.0",
    "source-map-support": "^0.5.11",
    "ts-loader": "^5.3.3",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/silexlabs/Silex.git"
  },
  "keywords": [
    "online html page builder",
    "cloud based open source SaaS",
    "free website template builder",
    "live web page editor",
    "html5 web page editor",
    "free html5 editor",
    "free website builder with source code",
    "free cms for html website",
    "free website with html",
    "cms open source",
    "free online html editors",
    "open source online editor",
    "silex website",
    "silex website builder",
    "silex.me",
    "open source online website builder",
    "silex web",
    "free cms editor",
    "tuto silex",
    "website free builder",
    "html5 website builder online",
    "silex css",
    "cms flash open source",
    "website builder with html editor",
    "free website builder sites",
    "silex cms",
    "online website builder open source",
    "Create a free website",
    "Wix.com open source alternative",
    "professional designer",
    "webdesigner, CSS web master",
    "webmaster CSS made easy",
    "open source CMS",
    "CSS3 HTML5&nbsp;HTML 5&nbsp;javascript editor",
    "WYSIWYG",
    "creative commons web templates",
    "creative templates",
    "github",
    "silex",
    "creative commons"
  ],
  "build": {
    "appId": "org.silexlabs.silex",
    "files": [
      "dist",
      "node_modules",
      "static",
      "package.json"
    ],
    "mac": {
      "category": "public.app-category.developer-tools",
      "target": "dmg"
    },
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "category": "Network",
      "icon": "build/icons/",
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "win": {
      "target": "squirrel",
      "icon": "build/icon.ico"
    },
    "nsis": {
      "createDesktopShortcut": "always"
    }
  }
}
